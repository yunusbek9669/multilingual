{
    "name": "yunusbek/yii2-multilingual",
    "description": "A multilingual package for PHP projects",
    "type": "library",
    "license": "MIT",
    "require": {
        "yiisoft/yii2": "^2.0",
        "phpoffice/phpspreadsheet": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Yunusbek\\Multilingual\\": "src/"
        }
    },
    "authors": [
        {
            "name": "yunusbek",
            "email": "inomovyunusbek@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
